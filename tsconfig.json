  "compilerOptions": {
    "outDir": "./dist/",
    "skipLibCheck": true,
    "noImplicitAny": true,
    "removeComments": true,
    "forceConsistentCasingInFileNames": true,
    "module": "commonjs",
    "target": "es5",
    "lib": ["dom", "es6", "es2015"],
    "jsx": "react",
    "allowJs": true
  }
}
